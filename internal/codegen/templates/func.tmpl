{{if .Implement}}
    func {{if and .FuncOwner (not .RequiresActivation)}}
        (v *{{.FuncOwner}})
    {{- end -}}

    {{funcName .}}

    {{- /* input params */ -}}

    (
    {{- range .InParams -}}
        {{.Name}} {{.Type}},
    {{- end -}}
    )

    {{- /* return params */ -}}

    (
    {{- if .ReturnParam -}}
        {{.ReturnParam.Type}},
    {{- end -}}
    error)


    {{- /* method body */ -}}

    {
    {{template "funcimpl.tmpl" .}}
    }
{{end}}
