{{if .Implement}}
    func {{if and .FuncOwner (not .RequiresActivation)}}
        (v *{{.FuncOwner}})
    {{- end -}}

    {{funcName .}} 
    
    {{- /* in params */ -}}

    ( 
    {{- range .InParams -}}
        {{.GoVarName}} {{ if .IsOut }}*{{ end -}}
        {{template "variabletype.tmpl" . }},
    {{- end -}}
    )

    {{- /* return params */ -}}

    ( {{range .ReturnParams}}{{template "variabletype.tmpl" . }},{{end}} error )

    {{- /* method body */ -}}

    {
    {{template "funcimpl.tmpl" .}}
    }
{{end}}
